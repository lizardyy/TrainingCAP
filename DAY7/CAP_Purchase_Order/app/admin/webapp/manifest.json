{
    "_version": "1.8.0",
    "sap.app": {
        "id": "admin",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "dataSources": {
            "AdminService": {
                "uri": "/api/admin/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui5": {
        "dependencies": {
            "libs": {
                "sap.fe.templates": {}
            },
            "minUI5Version": "1.122.1"
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "admin.i18n.i18n"
                }
            },
            "": {
                "dataSource": "AdminService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "POList",
                    "target": "POList"
                },
                {
                    "pattern": "Orders({key}):?query:",
                    "name": "POObjectPage",
                    "target": "POObjectPage"
                }
            ],
            "targets": {
                "POList": {
                    "type": "Component",
                    "id": "POList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Orders",
                            "navigation": {
                                "Orders":{
                                    "detail":{
                                        "route": "POObjectPage"
                                    }
                                }
                            },
                            "content": {
                                "header": {
                                    "actions": {
                                        
                                    }
                                }
                            },
                            "initialLoad": "Enabled",
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "selectAll": false,
                                        "selectionMode": "Multi"
                                    },
                                    "actions": {
                                    }
                                }
                            }
                        }
                    }
                },
                "POObjectPage":{
                    "type": "Component",
                    "id": "POObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "contextPath": "/Orders",
                            "navigation": {},
                            "content": {
                                "header": {
                                    "anchorBarVisible": true
                                }
                            },
                            "editableHeaderContent": false,
                            "sectionLayout": "Page"
                        }
                    }
                }            
                
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": false,
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    }
}